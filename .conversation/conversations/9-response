$(document).ready(function($) {
	$.get(
		"api/index.php",
		{ conf: "icse" },
		createSlider
	);
});

function createSlider(data) {
	mydata = data;
	for(var key in data) {
		var img = $("<img>")
			.attr("width", "400px")
			.attr("src", "img/icse/" + data[key]);
		$("#slider").append($("<li>").attr("id", key).append(img));
	}

	$("#slider").bxSlider({
		slideWidth: 400,
	});
}